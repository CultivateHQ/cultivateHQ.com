---
layout: false
---
<?xml version="1.0" encoding="UTF-8"?>

<% website_url = 'http://www.cultivatehq.com' %>

<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  <url>
    <loc><%= website_url %></loc>
    <lastmod><%= Date.today.to_time.iso8601 %></lastmod>
    <changefreq>daily</changefreq>
    <priority>1.0</priority>
  </url>
  <url>
    <loc><%= website_url %>/posts</loc>
    <lastmod><%= Date.today.to_time.iso8601 %></lastmod>
    <changefreq>daily</changefreq>
    <priority>1.0</priority>
  </url>

  <% sitemap.resources.select { |page| page.path =~ /posts\// && page.path != 'posts/index.html' && !page.data.noindex == true }.each do |page| %>
    <url>
      <loc><%= website_url %><%= page.url %></loc>
      <lastmod><%= Date.today.to_time.iso8601 %></lastmod>
      <changefreq>monthly</changefreq>
      <priority>0.5</priority>
    </url>
  <% end %>
</urlset>
